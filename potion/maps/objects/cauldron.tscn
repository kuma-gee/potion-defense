	float n_out11p0 = Shadow_Max;


// Clamp:10
	vec3 n_out10p0 = clamp(n_out5p0, vec3(n_out9p0), vec3(n_out11p0));


// Output:0
	DIFFUSE_LIGHT = n_out10p0;


}
